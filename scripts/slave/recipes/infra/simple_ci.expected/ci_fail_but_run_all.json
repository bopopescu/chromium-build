[
  {
    "cmd": [
      "git",
      "init"
    ],
    "name": "git init"
  },
  {
    "cmd": [
      "git",
      "reset",
      "--hard"
    ],
    "name": "git reset"
  },
  {
    "cmd": [
      "git",
      "fetch",
      "https://chromium.googlesource.com/infra/infra",
      "release-52"
    ],
    "name": "git fetch"
  },
  {
    "cmd": [
      "git",
      "checkout",
      "FETCH_HEAD"
    ],
    "name": "git checkout"
  },
  {
    "cmd": [
      "git",
      "submodule",
      "update",
      "--init",
      "--recursive"
    ],
    "name": "git submodule update"
  },
  {
    "cmd": [
      "python",
      "-u",
      "with open('infra/config/ci.cfg') as f: print f.read()"
    ],
    "name": "read tests",
    "stdout": "/path/to/tmp/",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@python.inline@with open('infra/config/ci.cfg') as f: print f.read()@@@",
      "@@@STEP_LOG_END@python.inline@@@"
    ]
  },
  {
    "cmd": [
      "./a.sh"
    ],
    "name": "test: ./a.sh",
    "~followup_annotations": [
      "step returned non-zero exit code: 1",
      "@@@STEP_FAILURE@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "b.py"
    ],
    "name": "test: python b.py"
  },
  {
    "cmd": [
      "python",
      "-u",
      "c.py",
      "args"
    ],
    "name": "test: python c.py args"
  },
  {
    "name": "$result",
    "reason": "1 out of 3 aggregated steps failed. Failures: Step('test: ./a.sh') failed with return_code 1",
    "status_code": 1
  }
]