[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::bot_update]/resources/bot_update.py",
      "--master",
      "client.dart",
      "--builder",
      "vm-linux-release-x64-asan-be",
      "--slave",
      "TestSlavename",
      "--spec",
      "cache_dir = '[ROOT]/b/build/slave/cache_dir'\nsolutions = [{'deps_file': 'DEPS', 'managed': False, 'name': 'sdk', 'url': 'https://chromium.googlesource.com/external/github.com/dart-lang/sdk.git'}]",
      "--root",
      "sdk",
      "--revision_mapping_file",
      "{}",
      "--git-cache-dir",
      "[ROOT]/b/build/slave/cache_dir",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "sdk@HEAD",
      "--force",
      "--output_manifest"
    ],
    "env": {
      "PATH": "%(PATH)s:RECIPE_PACKAGE_REPO[depot_tools]"
    },
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"sdk\": \"HEAD\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"sdk\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"sdk\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]/gclient.py",
      "runhooks"
    ],
    "env": {
      "PATH": "%(PATH)s:RECIPE_PACKAGE_REPO[depot_tools]"
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[CWD]/sdk/tools/task_kill.py",
      "--kill_browsers=True"
    ],
    "cwd": "[CWD]/sdk",
    "name": "taskkill before building"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[CWD]/sdk/tools/build.py",
      "-mrelease",
      "--arch=x64",
      "runtime"
    ],
    "cwd": "[CWD]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "CXX": "third_party/clang/linux/bin/clang++ -fsanitize=address -fPIC"
    },
    "name": "build dart"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[CWD]/sdk/tools/test.py",
      "-mrelease",
      "--arch=x64",
      "--progress=line",
      "--report",
      "--time",
      "--failure-summary",
      "--write-debug-log",
      "--write-test-outcome-log",
      "--copy-coredumps",
      "--builder-tag=asan",
      "--timeout=240",
      "--exclude-suite=pkg"
    ],
    "cwd": "[CWD]/sdk",
    "name": "vm tests"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[CWD]/sdk/tools/test.py",
      "-mrelease",
      "--arch=x64",
      "--progress=line",
      "--report",
      "--time",
      "--failure-summary",
      "--write-debug-log",
      "--write-test-outcome-log",
      "--copy-coredumps",
      "--builder-tag=asan",
      "--timeout=240",
      "--exclude-suite=pkg",
      "--checked"
    ],
    "cwd": "[CWD]/sdk",
    "name": "checked vm tests"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[CWD]/sdk/tools/task_kill.py",
      "--kill_browsers=True"
    ],
    "cwd": "[CWD]/sdk",
    "name": "taskkill after testing"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]