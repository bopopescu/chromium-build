      "[START_DIR]/skimage/nanobench",
      "--nocpu",
      "--dummy-flags"
    ],
    "env": {
      "BUILDTYPE": "Release",
      "CHROME_HEADLESS": "1",
      "GYP_DEFINES": "skia_arch_type=x86_64 skia_release_optimization_level=1 skia_warnings_as_errors=0",
      "PYTHONPATH": "RECIPE_PACKAGE_REPO[build]/scripts",
      "SKIA_OUT": "[START_DIR]/out"
    },
    "name": "nanobench"
  },
  {
    "cmd": [
      "valgrind",
      "--gen-suppressions=all",
      "--leak-check=full",
      "--track-origins=yes",
      "--error-exitcode=1",
      "--num-callers=40",
      "--suppressions=[START_DIR]/skia/tools/valgrind.supp",
      "[START_DIR]/out/Release/nanobench",
      "--undefok",
      "-i",
      "[START_DIR]/skia/resources",
      "--skps",
      "[START_DIR]/skp",
      "--images",
      "[START_DIR]/skimage/nanobench",
      "--nocpu",
      "--dummy-flags",
      "--abandonGpuContext",
      "--nocpu"
    ],
    "env": {
      "BUILDTYPE": "Release",
      "CHROME_HEADLESS": "1",
      "GYP_DEFINES": "skia_arch_type=x86_64 skia_release_optimization_level=1 skia_warnings_as_errors=0",
      "PYTHONPATH": "RECIPE_PACKAGE_REPO[build]/scripts",
      "SKIA_OUT": "[START_DIR]/out"
    },
    "name": "nanobench --abandonGpuContext"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]