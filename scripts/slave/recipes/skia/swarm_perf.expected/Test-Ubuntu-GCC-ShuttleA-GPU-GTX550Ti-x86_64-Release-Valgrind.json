      "@@@STEP_LOG_LINE@json.output@    \"nanobench\"@@@",
      "@@@STEP_LOG_LINE@json.output@  ], @@@",
      "@@@STEP_LOG_LINE@json.output@  \"builder_cfg\": {@@@",
