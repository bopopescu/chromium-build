[
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/skia/tools/buildbot_spec.py",
      "/path/to/tmp/json",
      "Test-Ubuntu-GCC-ShuttleA-GPU-GTX550Ti-x86_64-Release-Valgrind"
    ],
    "cwd": "[START_DIR]/skia",
    "infra_step": true,
    "name": "exec buildbot_spec.py",
