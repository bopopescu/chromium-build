      "@@@STEP_LOG_LINE@json.output@    \"role\": \"Test\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"configuration\": \"Release\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"dm_flags\": [@@@",
