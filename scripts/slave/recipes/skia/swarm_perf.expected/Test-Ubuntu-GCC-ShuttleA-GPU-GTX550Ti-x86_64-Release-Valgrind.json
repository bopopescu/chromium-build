      "@@@STEP_LOG_LINE@json.output@    \"compiler\": \"GCC\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"configuration\": \"Release\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"cpu_or_gpu\": \"GPU\", @@@",
