[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]/gclient.py",
      "config",
      "--spec",
      "cache_dir = '[ROOT]/b/build/slave/cache_dir'\nsolutions = [{'custom_vars': {'chromium_trunk': 'svn://svn-mirror.golo.chromium.org/chrome/trunk'}, 'deps_file': 'DEPS', 'managed': True, 'name': 'v8', 'url': 'https://chromium.googlesource.com/v8/v8'}]"
    ],
    "env": {
      "PATH": "%(PATH)s:RECIPE_PACKAGE_REPO[depot_tools]"
    },
    "name": "gclient setup"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]/gclient.py",
      "sync",
      "--nohooks",
      "--force",
      "--verbose",
      "--delete_unversioned_trees",
      "--with_branch_heads",
      "--output-json",
      "/path/to/tmp/json"
    ],
    "env": {
      "PATH": "%(PATH)s:RECIPE_PACKAGE_REPO[depot_tools]"
    },
    "name": "gclient sync",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"solutions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"v8/\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"revision\": 50110@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@50110@@@"
    ]
  },
  {
    "cmd": [
      "git",
      "checkout",
      "-f",
      "FETCH_HEAD"
    ],
    "cwd": "[CWD]/v8",
    "name": "git checkout"
  },
  {
    "cmd": [
      "git",
      "branch",
      "-D",
      "work"
    ],
    "cwd": "[CWD]/v8",
    "name": "git branch"
  },
  {
    "cmd": [
      "git",
      "clean",
      "-ffd"
    ],
    "cwd": "[CWD]/v8",
    "name": "git clean"
  },
  {
    "cmd": [
      "git",
      "show",
      "refs/remotes/branch-heads/3.4:include/v8-version.h"
    ],
    "cwd": "[CWD]/v8",
    "name": "Check latest version file",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "git",
      "show",
      "refs/remotes/branch-heads/3.4~1:include/v8-version.h"
    ],
    "cwd": "[CWD]/v8",
    "name": "Check previous version file",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "git",
      "log",
      "-n1",
      "--format=%H",
      "refs/remotes/branch-heads/3.4"
    ],
    "cwd": "[CWD]/v8",
    "name": "git log",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "git",
      "show",
      "74882b7a8e55268d1658f83efefa1c2585cee723:include/v8-version.h"
    ],
    "cwd": "[CWD]/v8",
    "name": "Check head version file",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "git",
      "describe",
      "--tags",
      "74882b7a8e55268d1658f83efefa1c2585cee723"
    ],
    "cwd": "[CWD]/v8",
    "name": "git describe",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "git",
      "ls-remote",
      "https://chromium.googlesource.com/v8/v8",
      "refs/heads/3.4-lkgr"
    ],
    "cwd": "[CWD]/v8",
    "name": "git ls-remote 3.4-lkgr",
    "stdout": "/path/to/tmp/",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@ref@74882b7a8e55268d1658f83efefa1c2585cee723@@@",
      "@@@STEP_LOG_END@ref@@@"
    ]
  },
  {
    "cmd": [
      "echo",
      "There is no new lkgr."
    ],
    "name": "log"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]