      "RECIPE_PACKAGE_REPO[depot_tools]/gsutil.py",
      "--ninja-path",
      "RECIPE_PACKAGE_REPO[depot_tools]/ninja",
      "--target",
      "Debug",
      "--src-dir",
      "[SLAVE_BUILD]/src",
      "--goma-cache-dir",
      "[GOMA_CACHE]",
      "--build-tool",
      "ninja",
      "--compiler",
      "goma-clang",
