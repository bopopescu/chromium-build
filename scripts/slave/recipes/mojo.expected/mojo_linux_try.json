      "--dcheck_always_on"
    ],
    "env": {
      "GOMA_DIR": "[CACHE]/cipd/goma"
    },
    "name": "mojob build"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/src/mojo/tools/get_test_list.py",
      "{\"build_number\": 571, \"builder_name\": \"Mojo Linux Try\", \"dcheck_always_on\": true, \"is_debug\": false, \"master_name\": \"chromium.testing.master\", \"target_os\": \"linux\", \"test_results_server\": \"test-results.appspot.com\", \"test_types\": [\"default\"]}",
      "/path/to/tmp/json"
    ],
    "name": "get_test_list",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@[@@@",
      "@@@STEP_LOG_LINE@json.output@  {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"command\": [@@@",
      "@@@STEP_LOG_LINE@json.output@      \"world\"@@@",
      "@@@STEP_LOG_LINE@json.output@    ], @@@",
      "@@@STEP_LOG_LINE@json.output@    \"name\": \"Hello\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@]@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "world"
    ],
    "cwd": "[SLAVE_BUILD]/src",
    "name": "Hello"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
