[
  {
    "cmd": [],
    "name": "ensure_goma"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::cipd]/resources/bootstrap.py",
      "--platform",
      "linux-amd64",
      "--dest-directory",
      "[START_DIR]/cipd",
      "--json-output",
      "/path/to/tmp/json"
    ],
    "name": "ensure_goma.install cipd",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_TEXT@cipd instance_id: 40-chars-fake-of-the-package-instance_id@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"executable\": \"[START_DIR]/cipd/cipd\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"instance_id\": \"40-chars-fake-of-the-package-instance_id\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "[START_DIR]/cipd/cipd",
      "ensure",
      "--root",
      "[CACHE]/cipd/goma",
      "--list",
      "infra/tools/cloudtail/linux-amd64 goma_recipe_module\ninfra_internal/goma/client/linux-amd64 release",
      "--json-output",
      "/path/to/tmp/json",
      "--service-account-json",
      "/creds/service_accounts/service-account-goma-client.json"
    ],
    "name": "ensure_goma.ensure_installed",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": [@@@",
      "@@@STEP_LOG_LINE@json.output@    {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"instance_id\": \"resolved-instance_id-of-goma_recipe_modu\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"package\": \"infra/tools/cloudtail/linux-amd64\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }, @@@",
      "@@@STEP_LOG_LINE@json.output@    {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"instance_id\": \"resolved-instance_id-of-release---------\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"package\": \"infra_internal/goma/client/linux-amd64\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  ]@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]/cros",
      "chrome-sdk",
      "--board=amd64-generic",
      "--nocolor",
      "--use-external-config",
      "--clear-sdk-cache",
      "--gomadir",
      "[CACHE]/cipd/goma",
      "--nostart-goma",
      "--",
      "python",
      "-u",
      "None/tools/mb/mb.py",
      "gen",
      "-m",
      "tryserver.chromium.linux",
      "-b",
      "Linux ChromiumOS Builder",
      "--config-file",
      "None/tools/mb/mb_config.pyl",
      "--goma-dir",
      "[CACHE]/cipd/goma",
      "--android-version-code=3",
      "--android-version-name=example",
      "//out_amd64-generic/Release"
    ],
    "cwd": "None",
    "env": {
      "GOMA_SERVICE_ACCOUNT_JSON_FILE": "/creds/service_accounts/service-account-goma-client.json"
    },
    "name": "generate_build_files"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[build]/scripts/tools/runit.py",
      "--show-path",
      "python",
      "RECIPE_MODULE[build::goma]/resources/utils.py",
      "jobs",
      "--file-path",
      "/path/to/tmp/"
    ],
    "name": "calculate the number of recommended jobs"
  },
  {
    "cmd": [],
    "name": "preprocess_for_goma"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[CACHE]/cipd/goma/goma_ctl.py",
      "restart"
    ],
    "env": {
      "GOMA_ALLOWED_NETWORK_ERROR_DURATION": "1800",
      "GOMA_CACHE_DIR": "[CACHE]/goma",
      "GOMA_DEPS_CACHE_FILE": "Linux_ChromiumOS_Builder.gomadeps",
      "GOMA_SERVICE_ACCOUNT_JSON_FILE": "/creds/service_accounts/service-account-goma-client.json"
    },
    "name": "preprocess_for_goma.start_goma",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[build::goma]/resources/cloudtail_utils.py",
      "start",
      "--cloudtail-path",
      "[CACHE]/cipd/goma/cloudtail",
      "--cloudtail-service-account-json",
      "/creds/service_accounts/service-account-goma-cloudtail.json",
      "--pid-file",
      "[TMP_BASE]/cloudtail.pid"
    ],
    "env": {
      "GOMA_SERVICE_ACCOUNT_JSON_FILE": "/creds/service_accounts/service-account-goma-client.json"
    },
    "name": "preprocess_for_goma.start cloudtail",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]/cros",
      "chrome-sdk",
      "--board=amd64-generic",
      "--nocolor",
      "--use-external-config",
      "--gomadir",
      "[CACHE]/cipd/goma",
      "--nostart-goma",
      "--",
      "RECIPE_PACKAGE_REPO[depot_tools]/ninja",
      "-w",
      "dupbuild=err",
      "-C",
      "None/out_amd64-generic/Release",
      "-j",
      "50",
      "All"
    ],
    "cwd": "None",
    "env": {
      "GOMA_SERVICE_ACCOUNT_JSON_FILE": "/creds/service_accounts/service-account-goma-client.json"
    },
    "name": "compile"
  },
  {
    "cmd": [],
    "name": "postprocess_for_goma"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[CACHE]/cipd/goma/goma_ctl.py",
      "jsonstatus"
    ],
    "env": {
      "GOMA_SERVICE_ACCOUNT_JSON_FILE": "/creds/service_accounts/service-account-goma-client.json"
    },
    "name": "postprocess_for_goma.goma_jsonstatus",
    "stdout": "/path/to/tmp/json",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_LOG_LINE@json.output@\"output of `goma_ctl.py json_status`\"@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "\nimport shutil\nimport sys\nshutil.copy(sys.argv[1], sys.argv[2])\n",
      "\"output of `goma_ctl.py json_status`\"",
      "[CACHE]/cipd/goma/jsonstatus"
    ],
    "name": "postprocess_for_goma.write_jsonstatus",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "[CACHE]/cipd/goma/goma_ctl.py",
      "stat"
    ],
    "env": {
      "GOMA_SERVICE_ACCOUNT_JSON_FILE": "/creds/service_accounts/service-account-goma-client.json"
    },
    "name": "postprocess_for_goma.goma_stat",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "[CACHE]/cipd/goma/goma_ctl.py",
      "stop"
    ],
    "env": {
      "GOMA_SERVICE_ACCOUNT_JSON_FILE": "/creds/service_accounts/service-account-goma-client.json"
    },
    "name": "postprocess_for_goma.stop_goma",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[build]/scripts/slave/upload_goma_logs.py",
      "--upload-compiler-proxy-info",
      "--json-status",
      "[CACHE]/cipd/goma/jsonstatus",
      "--ninja-log-outdir",
      "None/out_amd64-generic/Release",
      "--ninja-log-command",
      "['RECIPE_PACKAGE_REPO[depot_tools]/ninja', '-w', 'dupbuild=err', '-C', 'None/out_amd64-generic/Release', '-j', 50, 'All']",
      "--ninja-log-exit-status",
      "0",
      "--ninja-log-compiler",
      "goma",
      "--buildbot-buildername",
      "Linux ChromiumOS Builder",
      "--buildbot-mastername",
      "tryserver.chromium.linux",
      "--buildbot-slavename",
      "build1-a1"
    ],
    "env": {
      "GOMA_SERVICE_ACCOUNT_JSON_FILE": "/creds/service_accounts/service-account-goma-client.json"
    },
    "name": "postprocess_for_goma.upload_log",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[build::goma]/resources/cloudtail_utils.py",
      "stop",
      "--killed-pid-file",
      "[TMP_BASE]/cloudtail.pid"
    ],
    "name": "postprocess_for_goma.stop cloudtail",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]